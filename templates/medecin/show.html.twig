{% extends 'front.html.twig' %}

{% block title %}Medecin{% endblock %}

{% block edit %}
    <h1>Medecin</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ medecin.id }}</td>
            </tr>
            <tr>
                <th>Nom_med</th>
                <td>{{ medecin.nomMed }}</td>
            </tr>
            <tr>
                <th>Prenom_med</th>
                <td>{{ medecin.prenomMed }}</td>
            </tr>
            <tr>
                <th>Tel_med</th>
                <td>{{ medecin.telMed }}</td>
            </tr>
            <tr>
                <th>Mail_med</th>
                <td>{{ medecin.mailMed }}</td>
            </tr>
            <tr>
                <th>Specialite</th>
                <td>{{ medecin.specialite }}</td>
            </tr>
            <tr>
                <th>Diplome</th>
                <td>{{ medecin.diplome }}</td>
            </tr>
            <tr>
                <th>Photo</th>
                 <td>
            <img src="{{ asset('' ~ medecin.photo) }}"
              style="width:100px;height:100px;" />
          </td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_medecin_index') }}">back to list</a>

    <a href="{{ path('app_medecin_edit', {'id': medecin.id}) }}">edit</a>

    {{ include('medecin/_delete_form.html.twig') }}
{% endblock %}
