{% extends 'back.html.twig' %}



{% block labo %}

  <h1 style="color:DarkBlue;">
    Analyses
  </h1>
  


{{ form_start(formt) }}

{{ form_end(formt) }}

  <table class="table">
    <thead>
      <tr>
        <th>
          Id
        </th>
        <th>
          Date
        </th>
        <th>
          Resultat
        </th>
        <th>
          PDF
        </th>
        <th>
          Prix
        </th>
        <th>
          Labo
        </th>
        <th>
          actions
        </th>
      </tr>
    </thead>
    <tbody>
      {% for analyse in analyses %}
        <tr>
          <td>
            {{ analyse.id }}
          </td>
          <td>
            {{ analyse.date }}
          </td>
          <td>
            {{ analyse.resultat }}
          </td>
          <td>
            <a href="{{ asset('pdfA/' ~ analyse.image) }}">Voir PDF </a>

          </td>
          <td>
            {{ analyse.prix }}
          </td>
          <td>
            {{ analyse.laboo }}
          </td>
          <td>
            
            
          </td>
        </tr>
      {% else %}
        <tr>
          <td colspan="6">
            Aucun enregistrement trouvé
          </td>
        </tr>
      {% endfor %}
    </tbody>
  </table>

  <a href="{{ path('app_analyse_new') }}">Créer un nouveau </a>




<br>
<br>



{% endblock %}
